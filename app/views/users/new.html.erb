<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>

<div class="sign-up">
  <h2>Sign Up</h2>
    <%= form_for @user do |f| %>
      <%= f.label :name%>
      <%= f.text_field :name%>

      <%= f.label :email%>
      <%= f.text_field :email%>

      <%= f.label :username %>
      <%= f.text_field :username%>

      <%= f.label :password%>
      <%= f.password_field :password%>

      <%= f.label :password_confirmation%>
      <%= f.password_field :password_confirmation%>

      <%= f.submit %>
    <% end %>
</div>

<div class="sign-in">
  <h2>Sign In</h2>
</div>
